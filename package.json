  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "prepush": "CI=true npm test",
    "start-js": "react-scripts-ts-tw start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts-ts-tw build",
    "c": "react-scripts-ts-tw init",
    "test": "react-scripts-ts-tw test --env=jsdom",
    "eject": "react-scripts-ts-tw eject",
    "cover": "react-scripts-ts-tw test --env=jsdom --coverage"
